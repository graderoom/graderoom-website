            function resetCategory(element) {
                let classes = $(element)[0].className.split(" ");
                let classIndex = parseFloat(classes.filter(x => (x.match(/^.*?(?=\d+)/) || [])[0] === "resetCategory")[0].match(/\d+$/)[0]);
