                } else if (grade >= 69.5) {
                    return "C-";
                } else if (grade >= 67.5) {
                    return "D+";
                } else if (grade >= 62.5) {
                    return "D";
                } else if (grade >= 59.5) {
                    return "D-";
                }
                return "F";
            }

