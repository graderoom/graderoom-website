            function showClassInfo(index) {
                let className = _data[index].class_name;
                let teacherName = _data[index].teacher_name;
                let relData = relClassData[className];
                let description = relData.description;
                let classAverage = relData.classAverage;
                let userCount = relData.userCount;
                let teachers = relData.teachers;
                let classType = relData.classType;
                let ucClassType = relData.uc_csuClassType;
                let gradeLevels = relData.gradeLevels;
                let credits = relData.credits;

                let parts = [];
                let headers = ["Prerequisites:", "Pre-requisites/Comments:", "Teacher: Staff", "Staff: Staff", "Course Content:", "", "(Reviewed "];

                let startHeader = -1;
                let startBody = -1;
                let endBody = -1;
                for (let i = 0; i < headers.length; i++) {
                    if (startHeader == -1) {
                        startHeader = description.indexOf(headers[i]);
                        startBody = startHeader + headers[i].length;
                    }
                    if (startHeader == -1) {
                        continue;
                    }
                    if (i == headers.length - 1) {
                        continue;
                    } // This iteration is just to get the index of the "(Reviewed" part
                    endBody = description.indexOf(headers[i + 1]);
                    if (endBody == -1) {
                        continue;
                    }
                    parts.push(description.substring(startHeader, startBody).trim());
                    parts.push(description.substring(startBody, endBody).trim());
                    startHeader = -1;
                    startBody = -1;
                }

                let reviewed = description.substring(startHeader);
                let reviewedEnd = reviewed.indexOf(")");
                reviewed = reviewed.substring(0, reviewedEnd + 1);
                parts.push(reviewed);
                parts.push("");

                let ucApproval = description.substring(startHeader + reviewedEnd + 1);
                if (ucApproval.startsWith("*")) {
                    ucApproval = ucApproval.substring(1);
                }
                parts.push(ucApproval);
                parts.push("");

                let _description = "";
                let _thereIsData = false;
                for (let i = 0; i < parts.length; i++) {
                    if (parts[i].trim().length > 0) _thereIsData = true;
                    if (i % 2 == 0) {
                        // These are the headers
                        _description += `<p class="classInfo-description-header">${parts[i]}</p>`;
                    } else {
                        // Descriptions
                        _description += `<p class="classInfo-description-body">${parts[i]}</p>`;
                    }
                }

                let _teachers = "";
                for (let i = 0; i < teachers.length; i++) {
                    if (teachers[i] === teacherName) {
                        continue;
                    }
                    _teachers += `<li>${teachers[i]}</li>`;
                }

                let classInfoClassName = $("#classInfoName");
                let classInfoTeacher = $("#classInfoTeacher");
                let classInfoClassType = $("#classInfoClassType");
                let classInfoUCClassType = $("#classInfoUCClassType");
                let classInfoTeachersHeader = $("#classInfoOtherTeachersHeader");
                let classInfoTeachers = $("#classInfoOtherTeachers");
                let classInfoUserCount = $("#classInfoGraderoomUsers");
                let classInfoClassAverage = $("#classInfoAverage");
                let classInfoDescription = $("#classInfoDescription");
                let classInfoYears = $("#classInfoYears");
                let classInfoCredits = $("#classInfoCredits");

                classInfoClassName.text(className);
                if (teacherName) {
                    classInfoTeacher.show().text(teacherName);
                } else {
                    classInfoTeacher.hide();
                }
                switch (classType) {
                    case "honors":
