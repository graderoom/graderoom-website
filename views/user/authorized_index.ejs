                        case 9:
                            _yearsText += "Freshmen";
                            break;
                        case 10:
                            _yearsText += "Sophomores";
                            break;
                        case 11:
                            _yearsText += "Juniors";
                            break;
                        case 12:
                            _yearsText += "Seniors";
                            break;
                        default:
                            count--;
                            continue;
                    }
                    if (count > 2 && i < gradeLevels.length - 1) {
                        _yearsText += ",";
